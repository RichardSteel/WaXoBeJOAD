extends layout

block content
	#banner.page-header
		.row
			.col-lg-6
				h1 JOAD
					small &nbsp;Junior Olympic Archery Development
	.row
		.container
			.row
				.col-sm-6.col-md-4.col-md-offset-4
					h2.text-center.login-title Coach Sign In
						br
						.account-wall
							img.img-responsive.img-rounded(src='https://lh5.googleusercontent.com/-b0-k99FZlyE/AAAAAAAAAAI/AAAAAAAAAAA/eu7opA4byxI/photo.jpg?sz=60')
							br
							form(class='form-signin')
							input(type='text' class='form-control' placeholder='Coach First Name')
							input(type='password' class='form-control' placeholder='Password')
							button.btn.btn-lg.btn-primary.btn-block Sign in
							